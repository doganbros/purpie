{
  "name": "octopus",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/doganbros/octopus.git",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.1",
    "@nestjs/core": "^8.0.1",
    "@nestjs/platform-express": "^8.0.1",
    "@nestjs/swagger": "^5.0.0",
    "@nestjs/typeorm": "^8.0.1",
    "@sendgrid/mail": "^7.4.5",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "faker": "^5.5.3",
    "flat": "^5.0.2",
    "grommet": "^2.17.3",
    "grommet-icons": "^4.6.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.22",
    "nodemailer": "^6.6.2",
    "npm-merge-driver": "^2.3.5",
    "pg": "^8.6.0",
    "pm2": "^4.5.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "styled-components": "^5.3.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start:web": "PORT=3000 react-scripts start",
    "start:web:noport": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "analyze:server": "eslint server/.",
    "analyze:web": "eslint src/.",
    "fix:server": "eslint --fix server/.",
    "fix:web": "eslint --fix src/.",
    "start:server": "rimraf server/dist && nest build && pm2 start ecosystem.config.js --only API_LOCAL && pm2 logs",
    "server:build": "rimraf server/dist && nest build",
    "server:format": "prettier --write \"server/src/**/*.ts\"",
    "server:start": "nest start",
    "server:start:dev": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:start:prod": "node dist/main",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "cd server && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js --config ./ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.1",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-handlebars": "^5.3.1",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "eslint": "7.17.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "10.0.2",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.0",
    "prettier": "2.2.1",
    "sass": "^1.32.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
